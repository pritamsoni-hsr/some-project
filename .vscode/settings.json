{
  "protoc": {
    "compile_on_save": false,
    "options": [
      "-I=${workspaceRoot}/proto",
      "-I=${workspaceRoot}/includes/googleapis",
      "-I=${workspaceRoot}/includes/grpc-gateway"
    ],
    "path": "/usr/local/bin/protoc"
  },
  "python.formatting.blackArgs": [
    "--line-length=119",
    "--pyi",
    "--color",
    "--quiet"
  ],
  "python.linting.flake8Args": [
    "--max-line-length=119",
    "--ignore=E731"
  ],
  "cSpell.words": [
    "devsupport"
  ]
}
